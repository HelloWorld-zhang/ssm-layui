+<%@ page contentType="text/html;charset=UTF-8" language="java" isELIgnored="false" %>
+<html>
+<head>
+    <meta charset="utf-8">
+    <title>添加和修改</title>
+    <meta name="renderer" content="webkit">
+    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
+    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
+    <link rel="stylesheet" href="/layui/css/layui.css"  media="all">
+</head>
+<body>
+<form class="layui-form" action="" lay-filter="form">
+    <div class="layui-form-item">
+        <div class="layui-input-inline">
+            <input type="text" name="id" style="display: none" value="${teacher.id}">
+        </div>
+    </div>
+    <div class="layui-form-item">
+        <label class="layui-form-label">讲师名称</label>
+        <div class="layui-input-inline">
+            <input type="text" name="name" lay-verify="required" lay-reqtext="用户名是必填项，岂能为空？"
+                   placeholder="请输入" autocomplete="off" class="layui-input" value="${teacher.name}">
+        </div>
+    </div>
+    <div class="layui-form-item layui-form-text">
+        <label class="layui-form-label">讲师简介</label>
+        <div class="layui-input-block">
+            <textarea name="intro" placeholder="请输入讲师简介" class="layui-textarea">${teacher.intro}</textarea>
+        </div>
+    </div>
+    <div class="layui-form-item layui-form-text">
+        <label class="layui-form-label">讲师资历</label>
+        <div class="layui-input-block">
+            <textarea name="career" placeholder="请输入讲师资历" class="layui-textarea">${teacher.career}</textarea>
+        </div>
+    </div>
+    <div class="layui-form-item">
+        <label class="layui-form-label">讲师头衔</label>
+        <div class="layui-input-inline">
+            <select name="level" lay-filter="level">
+                <option value="1" ${teacher.level==1?'selected':''}>高级讲师</option>
+                <option value="2" ${teacher.level==2?'selected':''}>首席讲师</option>
+            </select>
+        </div>
+    </div>
+    <!--TODO 讲师头像-->
+    <div class="layui-form-item">
+        <label class="layui-form-label">创建时间</label>
+        <div class="layui-input-inline">
+            <input type="text" name="gmt_create" class="layui-input" id="gmt_create"
+                   placeholder="yyyy-MM-dd HH:mm:ss" value="${teacher.gmt_create}">
+        </div>
+    </div>
+    <div class="layui-form-item">
+        <label class="layui-form-label">更新时间</label>
+        <div class="layui-input-inline">
+            <input type="text" name="gmt_modified" id="gmt_modified"  placeholder="yyyy-MM-dd hh:mm:ss"
+                   class="layui-input" value="${teacher.gmt_modified}">
+        </div>
+    </div>
+    <div class="layui-form-item">
+        <div class="layui-input-block">
+            <button class="layui-btn" lay-submit lay-filter="submit">立即提交</button>
+        </div>
+    </div>
+</form>
+<script src="/layui/layui.js" charset="utf-8"></script>
+<script>
+    layui.use(['form',  'laydate'], function(){
+        var form = layui.form
+            ,layer = layui.layer
+            ,$=layui.$
+            ,laydate = layui.laydate;
+
+        //日期
+        laydate.render({
+            elem: '#gmt_create'
+            ,type: 'datetime'
+            ,format:'yyyy-MM-dd HH:mm:ss'
+            ,trigger: 'click'//呼出事件改成click
+        });
+        laydate.render({
+            elem: '#gmt_modified'
+            ,type: 'datetime'
+            ,format:'yyyy-MM-dd HH:mm:ss'
+            ,trigger: 'click'
+        });
+
+        form.on('submit(submit)',function(data){
+                //console.log(JSON.stringify(data.field));
+            $.ajax({
+                type:'POST',
+                url:'/edu/teacher/addOrUpdate.do',
+                contentType:"application/json;charset=UTF-8",
+                dataType:'json',
+                data: JSON.stringify(data.field),
+                success:function (res) {
+
+                    if(res.code==0){
+                        var table=parent.layui.table;
+                        parent.layer.msg(res.msg,{icon:6,time:2000});
+                        table.reload("test");
+                        var index=parent.layer.getFrameIndex(window.name);
+                        parent.layer.close(index);
+                    }
+                    else {
+                        layer.msg(res.msg,{icon:5,time:2000});
+                    }
+
+                }
+            });
+            return false;
+
+
+        });
+    });
+</script>
+</body>
+</html>
